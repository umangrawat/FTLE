<xml>
    <proxy>
        <group>filters</group>
        <name>FTLECUDA</name>
        <label>FTLE CUDA</label>
        <documentation>
            <brief>Computes FTLE on the given patch of the dataset</brief>
            <long>
                Computes Finite-Time Lyapunov Exponents on uniform grids.
            </long>
        </documentation>
        <property>
            <name>Input</name>
            <label>Input</label>
            <documentation>
                <brief/>
                <long>
                    Compute Finite-Time Lyapunov exponent, which characterizes the rate of seperatation of massless
                    tracer particles within a flow.
                </long>
            </documentation>
            <defaults/>
            <domains>
                <domain>
                    <text>Accepts input of following types:</text>
                    <list>
                        <item>vtkImageData</item>
                    </list>
                </domain>
                <domain>
                    <text>
      The dataset must contain a field array (point)
      
        with 3 component(s).
      </text>
                </domain>
            </domains>
        </property>
        <property>
            <name>Start time</name>
            <label>Start time</label>
            <documentation>
                <brief/>
                <long>Set initial advection time for the flow map computation</long>
            </documentation>
            <defaults>2.2</defaults>
            <domains/>
        </property>
        <property>
            <name>End time</name>
            <label>End time</label>
            <documentation>
                <brief/>
                <long>Set final advection time for the flow map computation</long>
            </documentation>
            <defaults>6.3</defaults>
            <domains/>
        </property>
        <property>
            <name>Maximum Number of Steps</name>
            <label>Maximum Number of Steps</label>
            <documentation>
                <brief/>
                <long>The maximum number of steps for the integrator.</long>
            </documentation>
            <defaults>10000</defaults>
            <domains/>
        </property>
        <property>
            <name>Seed grid origin, relative to input</name>
            <label>Seed grid origin, relative to input</label>
            <documentation>
                <brief/>
                <long>
                    Origin of seeding grid.
                </long>
            </documentation>
            <defaults>0 0 0</defaults>
            <domains/>
        </property>
        <property>
            <name>Seed grid bounds</name>
            <label>Seed grid bounds</label>
            <documentation>
                <brief/>
                <long>
                    Size of seeding grid.
                </long>
            </documentation>
            <defaults>20.0 10.0 0.0</defaults>
            <domains/>
        </property>
        <property>
            <name>Seed grid dimension</name>
            <label>Seed grid dimension</label>
            <documentation>
                <brief/>
                <long>
                    Resolution of seeding grid.
                </long>
            </documentation>
            <defaults>200.0 100.0 2.0</defaults>
            <domains/>
        </property>
        <property>
            <name>UseGPU</name>
            <label>UseGPU</label>
            <documentation>
                <brief/>
                <long>Use GPU for stream/path line integration, not yet implemented.</long>
            </documentation>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>ShowStreamLines(CPU only)</name>
            <label>ShowStreamLines(CPU only)</label>
            <documentation>
                <brief/>
                <long>Return streamlines within the multiBlock</long>
            </documentation>
            <defaults>0</defaults>
            <domains>
                <domain>
                    <text>Accepts boolean values (0 or 1).</text>
                </domain>
            </domains>
        </property>
        <property>
            <name>Integration step size</name>
            <label>Integration step size</label>
            <documentation>
                <brief/>
                <long>Set step size for the fixed step size RK4 integration scheme.</long>
            </documentation>
            <defaults>0.1</defaults>
            <domains/>
        </property>
    </proxy>
</xml>
